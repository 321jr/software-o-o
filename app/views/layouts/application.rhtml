        <div class="box_bottom_row">
         <div class="box_left"></div>
         <div class="box_right"></div>
        </div>
       </div>
       <br />
       <!-- End openSUSE navigation -->
       <!-- Begin custom navigation -->
       <div class="orange_box">
        <div class="box_top_row">
         <div class="box_left"></div>
         <div class="box_right"></div>
        </div>
        <div class="box_title_row">
         <div class="box_title">
          Getting Help
         </div>
        </div>
        <div class="box_content">
         <ul class="navlist">
          <li><a href="http://en.opensuse.org/Download_Help">Download Help</a></li>
          <li><a href="http://en.opensuse.org/Download_Help#Burn_the_ISO_Image.28s.29">Burn CD/DVD Image(s)</a></li>
          <li><a href="http://en.opensuse.org/Installation">Installation Guides</a></li>
          <li><a href="http://www.novell.com/documentation/opensuse111/">Official Manuals</a></li>
         </ul>
        </div>
        <div class="box_bottom_row">
         <div class="box_left"></div>
         <div class="box_right"></div>
        </div>
       </div>
       <br />
       <div class="orange_box">
        <div class="box_top_row">
         <div class="box_left"></div>
         <div class="box_right"></div>
        </div>
        <div class="box_title_row">
         <div class="box_title">
          Software
         </div>
        </div>
        <div class="box_content">
         <ul class="navlist">
          <li><%= link_to 'Software Search', {:controller => "search", :action => "index"} %></li>
          <li><a href="http://en.opensuse.org/Additional_YaST_Package_Repositories">Package Repositories</a></li>
          <li><a href="http://download.opensuse.org/update/">Update Repositories</a></li>
          <li><a href="http://download.opensuse.org/source/distribution/11.1/">Source Repository</a></li>
        </ul>
        </div>
        <div class="box_bottom_row">
         <div class="box_left"></div>
         <div class="box_right"></div>
        </div>
       </div>
       <br />
