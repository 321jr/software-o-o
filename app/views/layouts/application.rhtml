        <div class="box_top_row">
         <div class="box_left"></div>
         <div class="box_right"></div>
        </div>
        <div class="box_title_row">
         <div class="box_title">
          Other Options
         </div>
        </div>
        <div class="box_content">
         <ul class="navlist">
          <li><a href="http://en.opensuse.org/Mirror_Selection">Mirrors</a></li>
          <li><a href="http://download.opensuse.org/distribution/11.1/iso/">Metalinks</a></li>
          <li><%= link_to 'Latest Development Version', {:controller => "main", :action => "developer"} %></li>
          <li><a href="/old/11.0">openSUSE 11.0</a></li>
          <li><a href="/old/10.3">openSUSE 10.3</a></li>
          <li><a href="/expert.html">Expert view</a></li>
         </ul>
        </div>
        <div class="box_bottom_row">
         <div class="box_left"></div>
         <div class="box_right"></div>
        </div>
       </div>
