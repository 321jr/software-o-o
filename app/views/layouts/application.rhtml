             <div class="box_top_row">
              <div class="box_left"></div>
              <div class="box_right"></div>
             </div>
             <div class="box_title_row">
              <div class="box_title">
<!-- temporary disabled until translations exist
               <div id="langsel" class="hide">
                <form action="" name="langsel">
                 <select name="lang" onchange="langRedirect()">
                  <option value="">In other languages</option>
                  <option value="">Deutsch</option>
                  <option value="">Français</option>
                 </select>
                </form>
               </div>
-->
               <div id="page_actions">
                <b style="font-size:120%"><%=@page_title%></b>
               </div>
              </div>
             </div>
             <div class="box_content" id="bodyContent">
               <!-- Begin Content area-->
               <%= @content_for_layout %>
               <!-- End Content area -->
             </div>
             <div class="box_footer_row">
              <div id="page_footer" class="box_footer">
               <!-- Begin Footer -->
               This site uses the <a href="http://www.yaml.de">YAML</a> CSS framework.
               <br />
               <a href="http://en.opensuse.org/openSUSE:About">About openSUSE</a> | 
               <a href="http://en.opensuse.org/openSUSE:General_disclaimer">Legal information</a> | 
               <a href="mailto:webmaster@opensuse.org">Feedback</a>
               <br />
               <br />
               <a href="http://www.novell.com/linux/"><img src="http://static.opensuse.org/c/images/common/founded_novell.gif" alt="Founded by Novell" /></a>
               <!-- End Footer -->
              </div>
             </div>
             <div class="box_bottom_row">
              <div class="box_left"></div>
              <div class="box_right"></div>
             </div>
            </div>
           </div>
          </div>
          <!-- Begin right content column -->
          <div class="c25r">
           <div class="subcr">
             <% if @right_box == :search -%>
             <div class="orange_box">
               <div class="box_top_row">
                 <div class="box_left"></div>
                 <div class="box_right"></div>
               </div>
               <div class="box_title_row">
                 <div class="box_title">
                   Package Search
                 </div>
               </div>
             <div class="box_content" id="search_os">
             <div id="get_software">
                    <%= link_to "", {:controller => 'search'}, {:class => 'search_form', :style => 'display: block; width: 100%; height: 100px;', :title => "Search Build Service"}  %>
                    Search for and install additional software packages from the openSUSE Build Service.
             </div>
           </div>
           <div class="box_bottom_row">
             <div class="box_left"></div>
             <div class="box_right"></div>
           </div>
           </div>
                <br />
                <% else -%>

           <div class="orange_box">
        <div class="box_top_row">
          <div class="box_left"></div>
          <div class="box_right"></div>
        </div>
        <div class="box_title_row">
          <div class="box_title">
            Get openSUSE
          </div>
         </div>
         <div class="box_content" id="get_os">
            <div id="download_os">
                    <%= link_to "", {:controller => 'main', :action => :index}, {:class => 'download_os_link', :style => 'display: block; width: 100%; height: 100px;'}  %>
      </div>
    </div>
    <div class="box_bottom_row">
      <div class="box_left"></div>
          <div class="box_right"></div>
        </div>
      </div>
            <br />
            <% end -%>
            <div class="orange_box">
