         <div class="box_left"></div>
         <div class="box_right"></div>
        </div>
<!-- temporary disabled until translations exist
        <div class="box_title_row">
         <div class="box_title">
          In other languages 
         </div>
        </div>
-->
        <div class="box_content">
         <ul class="navlist">
          <li><a href="#">Deutsch</a></li>
          <li><a href="#">Français</a></li>
         </ul>
        </div>
        <div class="box_bottom_row">
         <div class="box_left"></div>
         <div class="box_right"></div>
        </div>
       </div>
       <br />
       </noscript>
       <!-- End Language navigation noscript -->
      </div>
     </div>
     <!-- End left column -->
     <!-- Begin right cloumn -->
     <div id="col3">
      <div id="col3_content" class="clearfix">
       <div class="orange_box_double" id="banner_orange">
        <div class="box_content_row">
         <div class="box_content" id="banner_content">
          <div id="slogan">
     <img src="http://static.opensuse.org/hosts/software.o.o/images/get.png" alt="Get it" />
          </div>
         </div>
        </div>
        <div class="box_bottom_row">
         <div class="box_left"></div>
         <div class="box_right"></div>
        </div>
       </div>
